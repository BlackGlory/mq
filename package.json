{
  "private": true,
  "name": "mq",
  "version": "0.7.0",
  "repository": "git@github.com:BlackGlory/mq.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --quiet src __tests__",
    "test": "vitest --run",
    "clean": "rimraf lib dist",
    "build": "tsc --project tsconfig.build.json",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag mq .",
    "docker:test": "run-s docker:test:clean docker:test:build docker:test:run",
    "docker:coverage": "run-s docker:test:clean docker:test:build docker:test:coverage docker:test:clean",
    "docker:test:build": "docker-compose --file docker-compose.test.yml build",
    "docker:test:run": "docker-compose --file docker-compose.test.yml run --rm test",
    "docker:test:coverage": "docker-compose --file docker-compose.test.yml run --rm coverage",
    "docker:test:clean": "docker-compose --file docker-compose.test.yml down",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare clean lint build test bundle smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/ms": "^2.1.0",
    "@types/node": "22.15.17",
    "@types/ws": "^8.18.1",
    "cross-env": "^7.0.2",
    "eslint": "^9.29.0",
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.0",
    "typescript-transform-paths": "^3.5.5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "yarn-deduplicate": "^6.0.2"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.19",
    "@blackglory/errors": "^3.0.3",
    "@blackglory/prelude": "^0.4.0",
    "@blackglory/structures": "^0.14.10",
    "@blackglory/wait-for": "^0.8.1",
    "@delight-rpc/websocket": "^0.7.4",
    "better-sqlite3": "^12.1.1",
    "bufferutil": "^4.0.9",
    "delight-rpc": "^6.1.2",
    "dotenv": "^16.5.0",
    "extra-abort": "^0.4.0",
    "extra-defer": "^0.3.1",
    "extra-filesystem": "^0.5.2",
    "extra-generator": "^0.5.9",
    "extra-json-stable-stringify": "^0.1.2",
    "extra-lazy": "^2.0.2",
    "extra-promise": "^7.0.0",
    "extra-semver": "^0.2.2",
    "extra-sort": "^0.1.2",
    "extra-tags": "^0.5.2",
    "extra-timers": "^0.2.6",
    "extra-utils": "^5.19.0",
    "iterable-operator": "^5.1.0",
    "justypes": "^4.4.1",
    "migration-files": "^0.4.3",
    "ms": "^2.1.3",
    "nanoid": "^5.1.5",
    "return-style": "^3.0.1",
    "utf-8-validate": "^6.0.5",
    "value-getter": "^0.3.3",
    "ws": "^8.18.2",
    "you-died": "^0.4.5"
  }
}
